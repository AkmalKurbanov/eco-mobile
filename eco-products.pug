      .header
        .mobile-container
          a(href="#").header__back.icon-back
          .header__title Эко продукты  
          a(href="#").basket.icon-group-533.notification
      .card-product
        .mobile-container 
          .swiper.product-js
            .swiper-wrapper
              mixin card(img)
                .swiper-slide
                  .card-product__item
                    .card-product__img
                      img(src="img/eco-product"+img+".jpg", alt="")
                    .card-product__name
                    .card-product__price
                    a(href="#").card-product__btn
                      img(src="img/ecoferma.svg")
                      | Ecoferma Market 
                    a(href="#").btn Подробнее  
              +card('1')
              +card('2')
              +card('3')
            .swiper-pagination  

      .card 
        .mobile-container 
          mixin card(img,title)
            .card__item
              .card__item-bg(style=`background-image: url('img/card${img}.jpg')`)
                .card__item-logo
                  img(src="img/logo-pattern.svg", alt="")
                .card__info 
                  .card__title!=title
                  div 
                    .card__rating
                      .rating-widget
                        .rating-stars.text-center
                          ul#stars
                            li.star(data-value='1')
                              i.icon-action-off
                            li.star(data-value='2')
                              i.icon-action-off
                            li.star(data-value='3')
                              i.icon-action-off
                            li.star(data-value='4')
                              i.icon-action-off
                            li.star(data-value='5')
                              i.icon-action-off
                    .card__subtitle цена за один день <br> 818 сом
          +card('1','Six Box')
          +card('2','Eat Fit Healthy')
          +card('3','Food Project')
          +card('4','Malina Eco Food')
          +card('5','Grow Food')
          +card('6','Healthy Food')

      
